---
import Link from "./Link.astro";

interface Props {
  class?: string;
  thickness: string;
  [key: string]: any;
}

const { href, class: classList, thickness, ...rest } = Astro.props;
---

<Link href={href} class:list={["underline-link", classList]} {...rest}>
  <slot />
</Link>

<style>
  .underline-link {
  }
</style>
